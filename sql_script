## Amount of rides for each day of the week for MEMBER, grouped by month_year.

##apr_20

SELECT 
"apr_20" AS month_year,
(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 1) AS  member_sunday_ride, 
    
(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 2) AS  member_monday_ride,

(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 3) AS  member_tuesday_ride,

(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 4) AS  member_wednesday_ride,

(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 5) AS  member_thursday_ride,

(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 6) AS  member_friday_ride,

(SELECT COUNT (day_of_week) 
FROM
    `deductive-wares-341003.nk_google.apr_20`
WHERE
    member_casual = "member" AND day_of_week = 7) AS member_saturday_ride; 
	
