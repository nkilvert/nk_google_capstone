        ###---QUERY---###

##Find top 10 most used start stations for most popular 3 months, for members: aug_20, sep_20, jul_20

SELECT 
	"aug_20" AS month_year, 
	"member" AS member_casual, 
    start_station_name, 
	COUNT(start_station_name) AS most_frequent, 
	start_lat, 
	start_lng
FROM 
    `deductive-wares-341003.nk_google.aug_20`
WHERE
    member_casual = "member"
GROUP BY start_station_name, start_lat, start_lng
ORDER BY COUNT(start_station_name) DESC
limit 10;

##

SELECT 
	"sep_20" AS month_year, 
	"member" AS member_casual, 
    start_station_name, 
	COUNT(start_station_name) AS most_frequent, 
	start_lat, 
	start_lng
FROM 
    `deductive-wares-341003.nk_google.sep_20`
WHERE
    member_casual = "member"
GROUP BY start_station_name, start_lat, start_lng
ORDER BY COUNT(start_station_name) DESC
limit 10;

##

SELECT 
	"jul_20" AS month_year, 
	"member" AS member_casual, 
